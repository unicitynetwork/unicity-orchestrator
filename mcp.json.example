{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "read_file",
        "write_file",
        "list_directory",
        "create_directory"
      ]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "disabled": false,
      "autoApprove": [
        "create_or_update_file",
        "create_pull_request",
        "get_issue"
      ]
    },
    "sqlite": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sqlite"],
      "env": {
        "SQLITE_DB_PATH": "./data.db"
      },
      "disabled": false,
      "autoApprove": [
        "read_query",
        "write_query",
        "create_table"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-brave-search"],
      "env": {
        "BRAVE_API_KEY": "${BRAVE_API_KEY}"
      },
      "disabled": false,
      "autoApprove": [
        "brave_search"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "navigate",
        "screenshot",
        "click",
        "type"
      ]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "create",
        "read",
        "update",
        "delete",
        "search",
        "list"
      ]
    },
    "fetch": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-fetch"],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "fetch"
      ]
    },
    "git": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-git"],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "git_clone",
        "git_log",
        "git_diff",
        "git_checkout",
        "git_commit",
        "git_push"
      ]
    }
  }
}